<!DOCTYPE html>
<head>
</head>
@{
        ViewBag.Title = "Home Page";
}
        @section sidebar{
<script type="text/javascript">
    function EditorialMouseOver() {
        mar = document.getElementById('EditorialSubMargin');
        mar.hidden = false;
        mar.style.visibility = "visible";
        el = document.getElementById('EditorialSub');
        el.hidden = false;
        el.style.visibility = "visible";
    }
    function EditorialMouseOut() {
        mar = document.getElementsById('EditorialSubMargin');
        mar.hidden = true;
        mar.style.visibility = "collapse";
        el = document.getElementsById('EditorialSub');
        el.hidden = true;
        el.style.visibility = "collapse";
    }
            </script>
        <section class="sidebar">
            <table >
                <tr style="height: 25px; border-bottom: 1px solid #333333;">
                    <td/><td/>
                </tr>
                <tr id="Editorial" style="position: relative; border-bottom: 1px solid #333333; height: 100px; width: 100%;" onmouseover="EditorialMouseOver()" onmouseout="EditorialMouseOut()">
                    <td style="width: 5%; height:100%; border-bottom: 1px solid #333333; padding: 0; margin: 0;">
                        <table>
                            <tr style="height:70px;">
                                <td />
                            </tr>
