<!DOCTYPE html>
<head>
</head>
@{
        ViewBag.Title = "Home Page";
}

<script type="text/javascript">
    function EditorialMouseOver() {
        mar = document.getElementById('EditorialSubMargin');
        mar.hidden = false;
        el = document.getElementById('EditorialSub');
        el.hidden = false;
    }

    function EditorialMouseOut() {
        mar = document.getElementsById('EditorialSubMargin');
        mar.hidden = true;
        el = document.getElementsById('EditorialSub');
        el.hidden = true;
    }
</script>
    @section featured {
        <section>

            <link rel="stylesheet" href="../../Scripts/Slideshow/themes/default/default.css" type="text/css" media="screen" />
            <link rel="stylesheet" href="../../Scripts/Slideshow/nivo-slider.css" type="text/css" />
            <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js" type="text/javascript"></script>
            <script src="../../Scripts/Slideshow/jquery.nivo.slider.js" type="text/javascript"></script>
            <script type="text/javascript">
                $(window).load(function () {
                    $('#slider').nivoSlider();
                });
            </script>
            <div id="wrapper">
                <div class="slider-wrapper theme-default">
                    <div id="slider" class="nivoSlider">
                        <img src="../../Content/Images/LaurenImages/Slideshow/Dashboard/slide1.jpg" alt="" />
                        <img src="../../Content/Images/LaurenImages/Slideshow/Dashboard/slide2.jpg" alt="" />
                        <img src="../../Content/Images/LaurenImages/Slideshow/Dashboard/slide3.jpg" alt="" />
                        <img src="../../Content/Images/LaurenImages/Slideshow/Dashboard/slide4.jpg" alt="" />
                    </div>
                </div>
            </div>
            <div id="content">
                <table style="width:97%;">
                    <tr style="height: 50px; width:100%;">
                        <td style="width:1%;"></td>
                        <td style=" border-bottom: 1px solid #333333;"></td>
                    </tr>
                    <tr style="height: 200px; width: 100%;">
                        <td style="width:1%;"></td>
                        <td style=" border-bottom: 1px solid #333333;"></td>
                    </tr>
                    <tr style="height:50px;"></tr>
                </table>
            </div>
</section>
        }
